				<string>on alfred_script(q)


set defaultBrowser to do shell script "defaults read \\
    ~/Library/Preferences/com.apple.LaunchServices/com.apple.launchservices.secure \\
    | awk -F'\"' '/http;/{print window[(NR)-1]}{window[NR]=$2}'"if defaultBrowser is "" or defaultBrowser contains "safari" then	--  # The default Browser is Safari.	set defaultBrowser to "Safari"	--  # Your code goes here.		tell application "Safari"		if it is not running then launch		delay 0.1		make new document		delay 0.1		activate			end tell		else if defaultBrowser contains "chrome" then	--  # The default Browser is Google Chrome.	set defaultBrowser to "Google Chrome"	--  # Your code goes here.	tell application "/Applications/Google Chrome.app"		make new window		activate	end tell	else if defaultBrowser contains "firefox" then	--  # The default Browser is Firefox.	set defaultBrowser to "Firefox"	--  # Your code goes here.		tell application "Firefox"		if it is not running then			launch		else			tell application "System Events"				tell process "Firefox"					click menu item "New Window" of menu "File" of menu bar 1				end tell			end tell		end if				delay 0.5		activate			end tell	else	set defaultBrowser to "Other"	--  # Your code goes here.end ifreturn defaultBrowser



end alfred_script</string>
